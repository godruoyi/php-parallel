{
    "name": "godruoyi/rust-php-extension-sample",
    "description": "A sample extension for PHP written in Rust",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "godruoyi",
            "email": "godruoyi@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
          "Godruoyi\\Composer\\": "/composer"
        }
    },
    "extra": {
        "class": "Godruoyi\\Composer\\InstallerPlugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
