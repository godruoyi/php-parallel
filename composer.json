{
    "name": "godruoyi/rust-php-ext-sample",
    "license": "MIT",
    "type": "composer-plugin",
    "extra": {
        "class": "OpenDAL\\Composer\\InstallerPlugin"
    },
    "autoload-dev": {
        "psr-4": {
            "Parallel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Godruoyi",
            "email": "godruoyi@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "*",
        "laravel/pint": "^1.18"
    },
    "scripts": {
        "test": "cargo build && php -d extension=target/debug/libphp_parallel_rust.dylib vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "php-ext": {
        "extension-name": "php_parallel",
        "description": "PHP extension for parallel computing"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
